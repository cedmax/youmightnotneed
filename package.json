{
  "name": "react-static-starter",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "new": "node modules/new",
    "start": "react-static start",
    "stage": "react-static build --staging",
    "build": "react-static build",
    "serve": "serve dist -p 3000",
    "test": "eslint src && jest --verbose"
  },
  "dependencies": {
    "axios": "0.18.0",
    "debounce": "1.2.0",
    "eslint-config-prettier": "4.2.0",
    "html-loader": "0.5.5",
    "jest": "23.0.0",
    "lodash": "4.17.11",
    "markdown-loader": "2.0.2",
    "marked": "0.6.2",
    "milligram": "1.3.0",
    "moment": "2.24.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-highlight.js": "1.0.7",
    "react-router": "4.2.0",
    "react-static": "5.1.14",
    "styled-components": "3.1.6"
  },
  "devDependencies": {
    "babel-plugin-styled-components": "1.5.0",
    "eslint-config-react-tools": "1.1.7",
    "husky": "2.2.0",
    "inquirer": "6.3.1",
    "lint-staged": "8.1.6",
    "prettier": "1.17.0",
    "serve": "11.0.0"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test"
    }
