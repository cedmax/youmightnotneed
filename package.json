{
  "name": "react-static-starter",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/cedmax/youmightnotneed",
  "license": "MIT",
  "scripts": {
    "new": "node modules/new",
    "contributors": "node modules/contributors",
    "start": "react-static start",
    "stage": "react-static build --staging",
    "build": "react-static build",
    "serve": "serve dist -p 3000",
    "pretest": "eslint src && eslint modules",
    "test": "jest --verbose",
    "posttest": "codecov --disable=gcov"
  },
  "dependencies": {
    "@reach/router": "^1.3.3",
    "axios": "0.19.2",
    "debounce": "1.2.0",
    "eslint-config-prettier": "6.10.0",
    "html-loader": "0.5.5",
    "jest": "25.1.0",
    "lodash": "4.17.15",
    "markdown-loader": "5.1.0",
    "marked": "0.8.0",
    "milligram": "1.3.0",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-highlight.js": "1.0.7",
    "react-hot-loader": "4.12.19",
    "react-router-dom": "5.1.2",
    "react-simple-linkify": "1.0.3",
    "react-static": "7.2.3",
    "react-static-plugin-styled-components": "7.2.2",
    "react-universal-component": "4.0.1",
    "styled-components": "5.0.1"
  },
  "devDependencies": {
    "codecov": "3.6.5",
    "babel-plugin-styled-components": "1.10.7",
    "cheerio": "^1.0.0-rc.3",
    "contributor-faces": "1.0.3",
    "eslint-config-react-tools": "1.1.7",
    "husky": "4.2.3",
    "inquirer": "7.0.5",
    "lint-staged": "10.0.8",
    "prettier": "1.19.1",
    "serve": "11.3.0"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test"
    }
  },
  "jest": {
    "coverageProvider": "v8",
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "./src/content/**/*.js"
    ]
  }
}
