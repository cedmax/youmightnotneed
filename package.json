    "inquirer": "7.3.3",
    "jest-date-mock": "1.0.8",
    "lint-staged": "12.3.3",
    "prettier": "2.5.1",
    "require-markdown": "1.2.0",
    "serve": "13.0.2"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint"
    ]
