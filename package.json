    "is-mobile": "3.0.0",
    "jest": "27.4.7",
    "lodash": "4.17.21",
    "markdown-loader": "5.2.0",
    "marked": "2.1.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-highlight": "0.12.0",
    "react-hot-loader": "4.13.0",
    "react-modal": "^3.14.4",
    "react-router-dom": "5.3.0",
    "react-simple-linkify": "1.0.3",
    "react-static": "7.6.2",
    "react-static-plugin-reach-router": "^7.6.2",
