{
  "extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
  "env": {
    "jest": true,
    "browser": true,
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "react/display-name": 0,
    "react/prop-types": 0,
    "import/no-extraneous-dependencies": 0
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
